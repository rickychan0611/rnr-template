{
  "expo": {
    "name": "Starter Base",
    "slug": "starter-base",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "fgImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/MiSans-Thin.ttf",
            "./src/assets/fonts/MiSans-ExtraLight.ttf",
            "./src/assets/fonts/MiSans-Light.ttf",
            "./src/assets/fonts/MiSans-Normal.ttf",
            "./src/assets/fonts/MiSans-Regular.ttf",
            "./src/assets/fonts/MiSans-Medium.ttf",
            "./src/assets/fonts/MiSans-Semibold.ttf",
            "./src/assets/fonts/MiSans-Demibold.ttf",
            "./src/assets/fonts/MiSans-Bold.ttf",
            "./src/assets/fonts/MiSans-Heavy.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}